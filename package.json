{
    "name": "beaglev2",
    "version": "1.0.0",
    "description": "Open source discord bot",
    "main": "./src/main.js",
    "scripts": {
        "start": "node ./dist/src/main.js",
        "prestart": "tsc --pretty --resolveJsonModule"
    },
    "author": "Cheese_strat",
    "license": "ISC",
    "dependencies": {
        "@discordjs/rest": "^0.3.0",
        "@discordjs/voice": "^0.8.0",
        "discord-api-types": "^0.27.3",
        "discord.js": "^14.6.0",
        "erela.js": "^2.3.3",
        "erela.js-spotify": "^1.2.0",
        "fs": "^0.0.1-security",
        "jimp": "^0.16.2",
        "libsodium-wrappers": "^0.7.9",
        "node-html-parser": "^6.1.1",
        "spotify-web-api-node": "^5.0.2",
        "typescript": "^4.5.5"
    },
    "repository": "https://github.com/Cheese-strat/BeagleV2",
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "embeddedLanguageFormatting": "off",
        "endOfLine": "crlf",
        "htmlWhitespaceSensitivity": "strict",
        "jsxBracketSameLine": false,
        "jsxSingleQuote": false,
        "parser": "typescript",
        "printWidth": 160,
        "proseWrap": "always",
        "quoteProps": "consistent",
        "semi": true,
        "singleQuote": false,
        "tabWidth": 4,
        "trailingComma": "all",
        "useTabs": true
    },
    "devDependencies": {
        "@types/node": "^18.11.9",
        "@types/spotify-web-api-node": "^5.0.7"
    }
}
